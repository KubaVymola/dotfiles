{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "options": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 ",
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}",
          "foreground": "#f0efe7",
          "background": "#9f1359",
          "type": "git",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "foreground": "#0fba81",
          "type": "time",
          "style": "plain"
        },
        {
          "options": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ .Code }} ",
          "foreground": "#ffffff",
          "background": "#00897b",
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if .Error }}#e91e63{{ end }}"
          ]
        },
        {
          "options": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 0
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ .FormattedMs }}",
          "foreground": "#222222",
          "background": "#76a8f9",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue753 {{ .Full }}",
          "foreground": "#000000",
          "background": "#1976d2",
          "type": "nx",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718 {{ .Full }}",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "type": "node",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue71e {{ .Full }}",
          "foreground": "#FFFFFF",
          "background": "#CB3837",
          "type": "npm",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue626 {{ .Full }}",
          "foreground": "#ffffff",
          "background": "#7FD5EA",
          "type": "go",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }} ({{ if .Venv }}{{ .Venv }}{{ end }}){{ end }}",
          "foreground": "#100e23",
          "background": "#906cff",
          "type": "python",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue7a8 {{ .Full }}",
          "foreground": "#193549",
          "background": "#99908a",
          "type": "rust",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " ",
          "foreground": "#222222",
          "background": "#ffcc88",
          "type": "root",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "foreground": "#aaaaaa",
          "background": "#252525",
          "type": "session",
          "style": "diamond"
        },
        {
          "options": {
            "_folder_icon": "\uf07b",
            "home_icon": "\ueb06",
            "max_width": 40,
            "style": "powerlevel"
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Path }}{{ if matchP \"^~$\" .PWD }} {{ end }}",
          "foreground": "#cccccc",
          "background": "#444444",
          "type": "path",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "$",
              "cmd": ">",
              "fish": "~>",
              "lua": ">",
              "nu": ":)",
              "pwsh": "❯",
              "shell": "❯",
              "zsh": "$"
            }
          },
          "template": " {{ .Name }} ",
          "foreground": "#007ACC",
          "type": "shell",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 4
}
