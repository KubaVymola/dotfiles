{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "type": "rprompt",
            "alignment": "right",
            "newline": true,
            "segments": [
                {
                    "background": "#9f1359",
                    "foreground": "#f0efe7",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }} {{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}",
                    "properties": {
                        "fetch_status": false,
                        "fetch_upstream_icon": false,
                        "untracked_modes": {
                            "/Users/user/Projects/oh-my-posh/": "no"
                        },
                        "branch_icon": "\ue725 ",
                        "cherry_pick_icon": "\ue29b ",
                        "commit_icon": "\uf417 ",
                        "merge_icon": "\ue727 ",
                        "no_commits_icon": "\uf0c3 ",
                        "rebase_icon": "\ue728 ",
                        "revert_icon": "\uf0e2 ",
                        "tag_icon": "\uf412 "
                    },
                    "style": "plain",
                    "_template": " {{ .HEAD }} ",
                    "type": "git"
                }
            ]
        },
        {
            "alignment": "left",
            "segments": [
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#0fba81",
                    "properties": {
                        "time_format": "15:04:05"
                    }
                },
                {
                    "type": "status",
                    "style": "accordion",
                    "foreground": "#ffffff",
                    "background": "#00897b",
                    "background_templates": ["{{ if .Error }}#e91e63{{ end }}"],
                    "trailing_diamond": "",
                    "template": " {{ .Code }} ",
                    "properties": {
                        "always_enabled": true
                    }
                },
                {
                    "background": "#76a8f9",
                    "foreground": "#222222",
                    "properties": {
                        "style": "austin",
                        "threshold": 0
                    },
                    "style": "plain",
                    "template": " {{ .FormattedMs }} ",
                    "type": "executiontime"
                },
                {
                    "type": "_nx",
                    "style": "powerline",
                    "_powerline_symbol": "",
                    "foreground": "#000000",
                    "background": "#1976d2",
                    "template": "  {{ .Full }} "
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "_powerline_symbol": "",
                    "foreground": "#ffffff",
                    "background": "#6CA35E",
                    "template": "  {{ .Full }} "
                },
                {
                    "type": "_npm",
                    "style": "powerline",
                    "_powerline_symbol": "",
                    "foreground": "#193549",
                    "background": "#ffeb3b",
                    "template": " {{ .Full }} "
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "background": "#ffcc88",
                    "foreground": "#222222",
                    "style": "plain",
                    "template": " ",
                    "type": "root"
                },
                {
                    "background": "#222222",
                    "foreground": "#888888",
                    "style": "plain",
                    "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
                    "type": "session"
                },
                {
                    "background": "#444444",
                    "foreground": "#cccccc",
                    "properties": {
                        "folder_icon": "\uf07b",
                        "_style": "powerlevel",
                        "style": "agnoster_short",
                        "home_icon": "\ueb06",
                        "max_depth": 2
                    },
                    "style": "plain",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "#7014eb",
                    "foreground": "#ffffff",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": " \ue77f ",
                    "type": "dotnet"
                },
                {
                    "background": "#7FD5EA",
                    "foreground": "#ffffff",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": " \ue626 ",
                    "type": "go"
                },
                {
                    "background": "#906cff",
                    "foreground": "#100e23",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": " \ue235 ",
                    "type": "python"
                },
                {
                    "background": "#99908a",
                    "foreground": "#193549",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": " \ue7a8 ",
                    "type": "rust"
                },
                {
                    "foreground": "#007ACC",
                    "style": "plain",
                    "template": " {{ .Name }} ",
                    "type": "shell",
                    "properties": {
                        "mapped_shell_names": {
                            "pwsh": "\u276f",
                            "shell": "\u276f",
                            "cmd": ">",
                            "lua": ">",
                            "nu": ":)",
                            "fish": "~>",
                            "zsh": "$",
                            "bash": "$"
                        }
                    }
                }
            ],
            "type": "prompt"
        }
    ],
    "version": 2
}
